{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "Tauri Plugins Example",
    "version": "0.1.0",
    "identifier": "com.example.plugins",
    "build": {
        "frontendDist": "dist"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "Tauri Plugins App",
                "width": 800,
                "height": 600
            }
        ]
    },
    "bundle": {
        "active": true,
        "targets": [
            "app",
            "dmg",
            "msi",
            "appimage",
            "deb"
        ],
        "icon": []
    }
}